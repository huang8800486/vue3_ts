{
	"name": "vite_vue3_ts",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"alpha": "vue-tsc --noEmit && vite build --mode alpha",
		"preview": "vite preview",
		"lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
		"prettier": "prettier --write .",
		"prepare": "husky install"
	},
	"dependencies": {
		"@vueuse/core": "^7.6.0",
		"vue": "^3.2.25"
	},
	"devDependencies": {
		"@types/node": "^17.0.16",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"@vitejs/plugin-vue": "^2.2.0",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.4.1",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.3",
		"mrm": "^3.0.10",
		"pinia": "^2.0.11",
		"prettier": "^2.5.1",
		"stylus": "^0.56.0",
		"stylus-loader": "^6.2.0",
		"typescript": "^4.5.4",
		"vite": "^2.8.0",
		"vue-i18n": "^9.1.9",
		"vue-tsc": "^0.29.8"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,vue,ts,tsx}": [
			"yarn lint",
			"prettier --write",
			"git add ."
		]
	}
}
